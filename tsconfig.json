{
  // https://www.typescriptlang.org/docs/handbook/compiler-options.html
  "compilerOptions": {
    "jsx": "react-native",
    "noUnusedParameters": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "noUnusedLocals": true,
    "module": "es6",
    "target": "es5",
    "moduleResolution": "node",
    "types": [
      "jest",
      "cypress"
    ],
    "lib": [
      "esnext",
      "dom"
    ],
    "baseUrl": "./App/prodApp/src",
    "paths": {
      "App": [
        "index.tsx"
      ],
      "@app": [
        "app"
      ],
      "@components": [
        "components"
      ],
      "@navigation": [
        "navigation/navigation"
      ],
      "@rnweb/*": [
        "lib/rnweb-light/*"
      ],
      "@screens/*": [
        "screens/*"
      ],
      "@screens": [
        "screens"
      ],
      "@state": [
        "state/state"
      ],
      "@logger": [
        "lib/logger"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@config": [
        "config"
      ],
      "@config/*": [
        "config/*"
      ],
      "actions": [
        "actions"
      ],
      "platform": [
        "config/platform"
      ],
      "uiUtils/*": [
        "utils/*"
      ],
    }
  }
}